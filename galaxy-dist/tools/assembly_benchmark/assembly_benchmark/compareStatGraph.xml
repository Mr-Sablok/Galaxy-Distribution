<tool id="compare_stat_graph" name="Compare stat-results">
  <description>graphically</description>
  <command interpreter="python">compareStatGraph.py
    $file $output</command>
  <inputs>
    <param format="txt" name="file" type="data" label="Stat file"/>
  </inputs>
  <outputs>
    <data format="html" name="output" from_work_dir="plot.html"/>
  </outputs>
  <help>
    This tool compares multiple stat-files and preview the result graphically.
  </help>

</tool>
